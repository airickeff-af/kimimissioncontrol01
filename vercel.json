{
  "version": 2,
  "name": "mission-control-dashboard",
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/logs/activity",
      "destination": "/api/logs/activity.js"
    },
    {
      "source": "/api/logs",
      "destination": "/api/logs/index.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1.js"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
