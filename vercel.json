{
  "version": 2,
  "rewrites": [
    { "source": "/office", "destination": "/dashboard/office.html" },
    { "source": "/agents", "destination": "/dashboard/agents.html" },
    { "source": "/tasks", "destination": "/dashboard/task-board.html" },
    { "source": "/deals", "destination": "/dashboard/deals.html" },
    { "source": "/tokens", "destination": "/dashboard/token-tracker.html" },
    { "source": "/logs", "destination": "/dashboard/logs.html" },
    { "source": "/data", "destination": "/dashboard/data.html" },
    { "source": "/standup", "destination": "/dashboard/standup.html" },
    { "source": "/", "destination": "/dashboard/index.html" },
    { "source": "/(.*)", "destination": "/dashboard/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "s-maxage=0" }
      ]
    }
  ]
}
