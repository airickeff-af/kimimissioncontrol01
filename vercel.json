{
  "version": 2,
  "name": "mission-control-fresh",
  "builds": [
    {
      "src": "mission-control/dashboard/**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/mission-control/dashboard/$1"
    },
    {
      "src": "/",
      "dest": "/mission-control/dashboard/index.html"
    }
  ]
}