{
  "version": 2,
  "routes": [
    { "src": "/", "dest": "/dashboard/index.html" },
    { "src": "/office", "dest": "/dashboard/office.html" },
    { "src": "/agents", "dest": "/dashboard/agents.html" },
    { "src": "/tasks", "dest": "/dashboard/task-board.html" },
    { "src": "/deals", "dest": "/dashboard/deals.html" },
    { "src": "/tokens", "dest": "/dashboard/token-tracker.html" },
    { "src": "/logs", "dest": "/dashboard/logs.html" },
    { "src": "/data", "dest": "/dashboard/data.html" },
    { "src": "/standup", "dest": "/dashboard/standup.html" },
    { "src": "/(.*)", "dest": "/dashboard/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "s-maxage=0" }
      ]
    }
  ]
}