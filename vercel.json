{
  "version": 2,
  "name": "mission-control-fresh",
  "builds": [
    {
      "src": "mission-control/dashboard/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "mission-control/dashboard/api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/logs/activity",
      "dest": "/mission-control/dashboard/api/logs-activity.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/logs-view.html",
      "dest": "/mission-control/dashboard/logs-view.html"
    },
    {
      "src": "/(.*)",
      "dest": "/mission-control/dashboard/$1"
    },
    {
      "src": "/",
      "dest": "/mission-control/dashboard/index.html"
    }
  ]
}
