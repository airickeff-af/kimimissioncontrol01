{
  "version": 2,
  "name": "mission-control-fresh",
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/logs-view.html",
      "destination": "/mission-control/dashboard/logs-view.html"
    },
    {
      "source": "/mission-control/dashboard/(.*)",
      "destination": "/mission-control/dashboard/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/mission-control/dashboard/$1"
    },
    {
      "source": "/",
      "destination": "/mission-control/dashboard/index.html"
    }
  ]
}
