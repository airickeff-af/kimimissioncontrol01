{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/logs/activity",
      "dest": "/api/logs/activity.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/agents",
      "dest": "/api/agents.js"
    },
    {
      "src": "/api/tasks",
      "dest": "/api/tasks.js"
    },
    {
      "src": "/api/deals",
      "dest": "/api/deals.js"
    },
    {
      "src": "/api/tokens",
      "dest": "/api/tokens.js"
    },
    {
      "src": "/api/test",
      "dest": "/api/test-simple.js"
    },
    {
      "src": "/api/logs",
      "dest": "/api/logs/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
