{
  "version": 2,
  "name": "mission-control-dashboard",
  "buildCommand": null,
  "outputDirectory": ".",
  "framework": null,
  "installCommand": null,
  "functions": {
    "api/**/*.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/logs/activity",
      "destination": "/api/logs-activity.js"
    },
    {
      "source": "/api/test",
      "destination": "/api/test.js"
    },
    {
      "source": "/api/logs",
      "destination": "/api/logs/index.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
