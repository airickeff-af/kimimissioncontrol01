{
  "version": 2,
  "rewrites": [
    { "source": "/office", "destination": "/office.html" },
    { "source": "/agents", "destination": "/agents.html" },
    { "source": "/tasks", "destination": "/task-board.html" },
    { "source": "/standup", "destination": "/standup.html" },
    { "source": "/token-tracker", "destination": "/token-tracker.html" },
    { "source": "/pie-radar", "destination": "/pie-radar.html" },
    { "source": "/logs", "destination": "/logs-view.html" },
    { "source": "/data", "destination": "/data-viewer.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "s-maxage=0" }
      ]
    }
  ]
}