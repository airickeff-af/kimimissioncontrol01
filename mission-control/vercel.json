{
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/agents", "dest": "/dashboard/agents.html" },
    { "src": "/agent-roster", "dest": "/dashboard/agents.html" },
    { "src": "/tasks", "dest": "/dashboard/task-board.html" },
    { "src": "/task-board", "dest": "/dashboard/task-board.html" },
    { "src": "/office", "dest": "/dashboard/office.html" },
    { "src": "/pixel-office", "dest": "/dashboard/office.html" },
    { "src": "/pixel", "dest": "/dashboard/office.html" },
    { "src": "/deals", "dest": "/dashboard/deals.html" },
    { "src": "/tokens", "dest": "/dashboard/tokens.html" },
    { "src": "/token-tracker", "dest": "/dashboard/token-tracker.html" },
    { "src": "/logs", "dest": "/dashboard/logs.html" },
    { "src": "/data", "dest": "/dashboard/data.html" },
    { "src": "/projects", "dest": "/dashboard/projects.html" },
    { "src": "/standup", "dest": "/dashboard/standup.html" },
    { "src": "/vrm", "dest": "/dashboard/vrm-viewer.html" },
    { "src": "/meebit", "dest": "/dashboard/vrm-viewer.html" },
    { "src": "/dashboard/(.*)", "dest": "/dashboard/$1" },
    { "src": "/(.*)", "dest": "/dashboard/index.html" }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "s-maxage=0" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type" }
      ]
    }
  ]
}
